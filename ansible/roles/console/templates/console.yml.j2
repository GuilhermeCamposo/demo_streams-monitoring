apiVersion: console.streamshub.github.com/v1alpha1
kind: Console
metadata:
  name: my-console
spec:
  hostname: my-console-{{ console_project }}.{{ route_subdomain }}
  kafkaClusters:
    - name: my-cluster
      namespace: {{ console_project }} 
      listener: console 
      properties:
        values: [] 
        valuesFrom: [] 
      credentials:
        kafkaUser:
          name: ui 